{% extends 'base.html.twig' %}

{% block title %}List of Posts{% endblock %}

{% block body %}
<div class="posts">
    <h1>List of Posts</h1>
    <ul>
        {% for post in posts %}
        <li>
            <h2>{{ post.title }}</h2>
            <p>{{ post.body }}</p>
            <p>Posted by: {{ post.userName }}</p>
        </li>
        {% endfor %}
    </ul>
</div>
{% endblock %}